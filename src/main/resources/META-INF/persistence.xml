<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
  <persistence-unit name="com.gihans_gs_war_1.0-SNAPSHOTPU" transaction-type="JTA">
    <jta-data-source>java:/jdbc/gs</jta-data-source>
    <!--    <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>-->
    <class>com.gihans.gs.model.Category</class>
    <class>com.gihans.gs.model.Brand</class>
    <class>com.gihans.gs.model.Item</class>
    <class>com.gihans.gs.model.Relation</class>
    <class>com.gihans.gs.model.RelatedItem</class>
    <class>com.gihans.gs.model.Relation_1</class>
    <class>com.gihans.gs.model.Brand_1</class>
    <class>com.gihans.gs.model.Item_1</class>
    <exclude-unlisted-classes>false</exclude-unlisted-classes>
    <properties>
      <property name="hibernate.dialect" value="org.hibernate.dialect.MySQLDialect"/>
    </properties>
  </persistence-unit>
</persistence>
